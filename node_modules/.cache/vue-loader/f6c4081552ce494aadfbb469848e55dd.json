/\"><i class=\"fas fa-tasks\"></i>Tarefas</router-link>\n        </div>\n        <div @click=\"emitFinish()\">\n          <router-link to=\"/\"><i class=\"far fa-check-circle\"></i>Finalizadas</router-link>\n        </div>\n        <div>\n          <router-link to=\"/\"><i class=\"fas fa-users\"></i>Sobre</router-link>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"NavBar\",\n  verifica: \"block\",\n  methods: {\n    emitTasks(){\n      this.$emit('allTasks')\n    },\n    emitFinish(){\n      this.$emit('finishedTasks')\n    },\n    \n    hamburger() {\n      const menu = document.getElementById(\"menu\");\n      const btn = document.getElementById('iconeBotao')\n      const verifica = menu.style.display;\n      this.verifica = verifica;\n      if (verifica == \"none\" || !verifica) {\n        menu.style.display = \"block\";\n        btn.classList.replace('fa-bars', 'fa-times');\n        btn.style.marginLeft = \"160px\"\n        btn.style.color = \"#000\"\n      } else {\n        menu.style.display = \"none\";\n        btn.classList.replace('fa-times', 'fa-bars');\n        btn.style.marginLeft = \"10px\"\n        btn.style.color = \"#fff\"\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n#menu {\n  display: none;\n  position: fixed;\n  top: 0;\n  left: 0;\n  height: 100%;\n  width: 150px;\n  background: #c8e1ff;\n  z-index: 100;\n  transition: all 0.5s ease;\n}\n\n#btn {\n  display: block;\n}\n\n#btn i {\n  position: fixed;\n  font-size: 40px;\n  top: 5px;\n  color: #fff;\n  z-index: 1001;\n  margin-left: 10px;\n  cursor: pointer;\n}\n.logo img {\n  width: 120px;\n  margin: 0px 0 25px 7px;\n}\n.links div {\n  margin: 10px 0 0 5px;\n  padding: 4px;\n  cursor: pointer;\n}\n\n.links div:hover {\n  background-color: #fff;\n}\n\n.links div a {\n  font-size: 17px;\n  color: #000;\n}\n.links div i {\n  margin-right: 10px;\n}\n\nbutton {\n  cursor: pointer;\n}\n\n</style>\n\n<style scoped>\n.header-top {\n\t